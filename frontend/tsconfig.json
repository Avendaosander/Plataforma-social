{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@/store*": ["./app/store*"],
      "@/lib*": ["./app/lib*"],
      "@/graphql*": ["./app/lib/graphql*"],
      "@/typesGraphql*": ["./app/lib/types/typesGraphql.ts"],
      "@/components*": ["./app/components*"],
      "@/ui*": ["./app/components/ui*"],
      "@/icons*": ["./app/components/icons/icons.ts"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "middleware.js", "next.config.mjs", "next-auth.d.ts"],
  "exclude": ["node_modules"]
}
